{
    "$schema": "https://raw.githubusercontent.com/moj-analytical-services/etl_manager/master/etl_manager/specs/table_schema.json",
    "name": "bookings",
    "description": "",
    "data_format": "parquet",
    "columns": [
        {
            "name": "id",
            "type": "character",
            "description": "Booking id"
        },
        {
            "name": "time_from",
            "type": "datetime",
            "description": "Start time of booking"
        },
        {
            "name": "time_to",
            "type": "datetime",
            "description": "End time of booking"
        },
        {
            "name": "created",
            "type": "datetime",
            "description": "Time the booking was created"
        },
        {
            "name": "cancelled_time",
            "type": "datetime",
            "description": "Time of cancellation"
        },
        {
            "name": "location_id",
            "type": "character",
            "description": "id to match to location"
        },
        {
            "name": "owner_id",
            "type": "character",
            "description": "id of user who owns the booking"
        },
        {
            "name": "booked_by_id",
            "type": "character",
            "description": "id of user who created the booking"
        },
        {
            "name": "status",
            "type": "character",
            "description": "One of APPROVED, TENTATIVE or CANCELLED"
        },
        {
            "name": "status_reason",
            "type": "character",
            "description": "Reason for cancellation where relevant"
        },
        {
            "name": "attendee_count",
            "type": "int",
            "description": "Self-reported number of attendees"
        }
    ],
    "location": "bookings"
}